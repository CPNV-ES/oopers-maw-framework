        "ext-pdo": "*"
    },
    "authors": [
        {
            "name": "dimitri-rutz",
            "email": "dimitri.rutz@eduvaud.ch"
        },
        {
            "name": "eliott-jaquier",
            "email": "eliott.jaquier@eduvaud.ch"
        }
    ],
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.26",
        "phpunit/phpunit": "^10",
        "saggre/phpdocumentor-markdown": "^0.1.3"
    }
}
